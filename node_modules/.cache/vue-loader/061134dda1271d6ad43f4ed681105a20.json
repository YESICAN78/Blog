"file":"Register.vue","sourceRoot":"src/components","sourcesContent":["<!-- Ê≥®ÂÜåÈ°µÈù¢ -->\r\n<template>\r\n  <el-dialog title=\"ÊèêÁ§∫\" :visible.sync=\"dialogVisible\" width=\"30%\" :before-close=\"beforeClose\">\r\n    <el-form ref=\"form\" :model=\"form\" label-width=\"80px\" :rules=\"rules\">\r\n      <el-form-item class=\"form-item\" label=\"Áî®Êà∑Âêç:\" prop=\"user\">\r\n        <el-input v-model=\"form.user\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item class=\"form-item\" label=\"ËæìÂÖ•ÂØÜÁ†Å\" prop=\"password\">\r\n        <el-input v-model=\"form.password\" show-password></el-input>\r\n      </el-form-item>\r\n      <el-form-item class=\"form-item\" label=\"Á°ÆËÆ§ÂØÜÁ†Å\" prop=\"checkpassword\">\r\n        <el-input v-model=\"form.checkpassword\" type=\"password\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item class=\"vcode\" label=\"È™åËØÅÁ†Å\" prop=\"vcode\">\r\n        <el-input v-model=\"form.vcode\"></el-input>\r\n        <div class=\"svg\" v-html=\"register.svgText\"></div>\r\n        <el-link\r\n          class=\"refresh\"\r\n          @click=\"getVocde\"\r\n          type=\"primary\"\r\n          :disabled=\"register.disabled\"\r\n        >{{register.refreshText}}</el-link>\r\n      </el-form-item>\r\n    </el-form>\r\n    <span slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button type=\"primary\" @click=\"handelColse\" :disabled=\"register.SubmitDisabled\">Á´ãÂç≥Ê≥®ÂÜå</el-button>\r\n    </span>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport request from \"../api/index\";\r\nconst getregistervcode = request.getregistervcode;\r\nconst getCheckVcode = request.getCheckVcode;\r\nconst postRegister = request.postRegister;\r\nexport default {\r\n  name: \"Register\",\r\n  data() {\r\n    return {\r\n      //formË°®ÂçïÁöÑÊï∞ÊçÆ\r\n      form: {\r\n        user: \"\",\r\n        password: \"\",\r\n        checkpassword: \"\",\r\n        vcode: \"\"\r\n      },\r\n      //Ë°®ÂçïÈ™åËØÅÊï∞ÊçÆ\r\n      rules: {\r\n        user: [\r\n          //triggerÂ§±ÂéªÁÑ¶ÁÇπËß¶Âèë\r\n          { required: true, message: \"ËØ∑ËæìÂÖ•Áî®Êà∑Âêç\", trigger: \"blur\" },\r\n          {\r\n            //Êï∞ÊçÆÁ±ªÂûã\r\n            type: \"string\",\r\n            //Ê≠£ÂàôËßÑÂàô\r\n            pattern: /^[\\w\\u4e00-\\u9fa5\\uac00-\\ud7ff\\u0800-\\u4e00\\-]{2,6}$/,\r\n            message: \"2-6‰ΩçÔºåÊï∞Â≠óÔºåÂ≠óÊØçÔºå_ -‰∏≠Êó•Èü©Êñá\",\r\n            trigger: [\"blur\", \"change\"]\r\n          }\r\n        ],\r\n        password: [\r\n          {\r\n            validator: (rule, value, callback) => {\r\n              if (value) {\r\n                if (\r\n                  /^[\\w<>,.?|;':\"{}!@#$%^&*()\\/\\-\\[\\]\\\\]{6,18}$/.test(value)\r\n                ) {\r\n                  callback();\r\n                } else {\r\n                  callback(new Error(\"ÂØÜÁ†Å6-18‰Ωç,‰∏çÂÖÅËÆ∏Âá∫Áé∞Â•áÊÄ™ÁöÑÂ≠óÁ¨¶\"));\r\n                }\r\n              } else {\r\n                callback(new Error(\"ËØ∑ËæìÂÖ•ÂØÜÁ†Å\"));\r\n              }\r\n              //Âú®ËøôÈáåËøòÈúÄË¶ÅÈ™åËØÅÁ°ÆËÆ§ÂØÜÁ†ÅÁöÑËÆ§ËØÅ\r\n              /**\r\n               * Â¶ÇÊûúÂú®vueÈáåÈù¢ÊÉ≥Ë¶ÅÊìç‰ΩúDOMÁöÑËØù\r\n               * Â∞±ÁªôÁà∂Á∫ßÊ†áÁ≠æ ËÆæÁΩÆ ref=\"name\"\r\n               * ÁÑ∂ÂêéÈÄöËøáthis.$refs.nameÁöÑÊñπÂºèËé∑Âèñ\r\n               * */\r\n\r\n              this.form.checkpassword &&\r\n                this.$refs.form.validateField(\"checkpassword\");\r\n            },\r\n            type: \"string\",\r\n            message: \"6-18‰ΩçÔºå‰∏çÂÖÅËÆ∏Âá∫Áé∞Â•áÊÄ™ÁöÑÂ≠óÁ¨¶\",\r\n            trigger: [\"blur\", \"change\"],\r\n            required: true\r\n          }\r\n        ],\r\n        checkpassword: [\r\n          { required: true, message: \"ÂÜçÊ¨°ËæìÂÖ•ÂØÜÁ†Å\", trigger: \"blur\" },\r\n          {\r\n            validator: (rule, value, callback) => {\r\n              if (value) {\r\n                if (value === this.form.password) {\r\n                  callback();\r\n                } else {\r\n                  callback(new Error(\"‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥\"));\r\n                }\r\n              } else {\r\n                callback(new Error(\"ËØ∑ÂÜçÊ¨°ËæìÂÖ•ÂØÜÁ†Å\"));\r\n              }\r\n            },\r\n            trigger: [\"blur\", \"change\"]\r\n          }\r\n        ],\r\n        //È™åËØÅÁ†ÅÈ™åËØÅ\r\n        vcode: [\r\n          {\r\n            validator: (rule, value, callback) => {\r\n              if (!value) {\r\n                callback(new Error(\"ËØ∑ËæìÂÖ•È™åËØÅÁ†Å\"));\r\n              } else {\r\n                let toUpperCase = value.toUpperCase();\r\n                //Â§±ÂéªÁÑ¶ÁÇπÈ™åËØÅÈ™åËØÅÁ†Å(ÂÜçÊ¨°ËØ∑Ê±Ç)\r\n                getCheckVcode(toUpperCase)\r\n                  .then(res => {\r\n                    if (res.data.code === 0) {\r\n                      callback();\r\n                    } else {\r\n                      callback(new Error(\"È™åËØÅÁ†ÅÈîôËØØ\"));\r\n                    }\r\n                  })\r\n                  .catch(e => {\r\n                    callback(new Error(\"Êú™Áü•ÁöÑÈîôËØØ\"));\r\n                  });\r\n              }\r\n            },\r\n            required: true,\r\n            trigger: [\"blur\", \"change\"]\r\n          }\r\n        ]\r\n      },\r\n      //Ê≥®ÂÜåÁõ∏ÂÖ≥ÁöÑÊï∞ÊçÆ\r\n      register: {\r\n        svgText: \"Loading...\",\r\n        refreshText: \"Âà∑Êñ∞\",\r\n        disabled: true,\r\n        timer: null,\r\n        //ÊåâÈíÆÁöÑÊòæÁ§∫ÂΩ±Ëóè\r\n        SubmitDisabled: false\r\n      }\r\n    };\r\n  },\r\n  props: [\"dialogVisible\"],\r\n  methods: {\r\n    //Ëé∑ÂèñÈ™åËØÅÁ†Å\r\n    getVocde() {\r\n      getregistervcode().then(res => {\r\n        //Ê∏ÖÈô§‰∏ä‰∏ÄÊ¨°‰øùÂ≠òÁöÑÊó∂Èó¥\r\n        clearTimeout(this.register.timer);\r\n        let _t = 0;\r\n        let fn = () => {\r\n          _t += 1000;\r\n          if (_t > res.data.time) {\r\n            clearTimeout(this.register.timer);\r\n            this.register.refreshText = \"Âà∑Êñ∞\";\r\n            this.register.disabled = false;\r\n          } else {\r\n            this.register.disabled = true;\r\n            this.register.refreshText =\r\n              (((res.data.time - _t) / 1000) | 0) + \"sÂêéÈáçËØï\";\r\n          }\r\n        };\r\n        this.register.timer = setInterval(fn, 1000);\r\n        fn();\r\n        this.register.svgText = res.data.data;\r\n      });\r\n    },\r\n    //Ê≥®ÂÜåÁÇπÂáª\r\n    handelColse() {\r\n      this.register.SubmitDisabled = true;\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n          //È™åËØÅÈÄöËøá\r\n          postRegister(this.form)\r\n            .then(res => {\r\n              //Êó†ËÆ∫ÊòØÊ≥®ÂÜåÂ§±Ë¥• ËøòÊòØÊàêÂäüÈÉΩË¶ÅÂà∑Êñ∞È™åËØÅÁ†Å\r\n              this.getVocde();\r\n              if (res.data.code) {\r\n                let msg = res.data.msg;\r\n                this.$message({\r\n                  message: res.data.msg,\r\n                  type: \"warning\",\r\n                  offset: 100,\r\n                  duration: 1500\r\n                });\r\n                //Ê∏ÖÈô§Ë°®ÂçïÊï∞ÊçÆ\r\n                this.$refs[\"form\"].resetFields();\r\n              } else {\r\n                this.$message({\r\n                  message: res.data.msg,\r\n                  type: \"success\",\r\n                  offset: 100,\r\n                  duration: 1500\r\n                });\r\n                this.register.SubmitDisabled = true;\r\n                //Ê≥®ÂÜåÊàêÂäüÂÖ≥Èó≠È°µÈù¢\r\n                setTimeout(() => {\r\n                  this.$emit(\"handelRegister\")\r\n                }, 1500);\r\n              }\r\n            })\r\n            .catch(e => {\r\n              this.getVocde();\r\n              console.log(e);\r\n            });\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    beforeClose() {\r\n      this.$confirm(\"Á°ÆÂÆöÂÖ≥Èó≠ü§£?\")\r\n        .then(() => {\r\n          this.$emit(\"handelRegister\");\r\n        })\r\n        .catch(() => {});\r\n    }\r\n  },\r\n  //ÁîüÂëΩÂë®Êúü - ÂàõÂª∫ÂÆåÊàêÔºàËÆøÈóÆÂΩìÂâçthisÂÆû‰æãÔºâ\r\n  created() {},\r\n  //ÁîüÂëΩÂë®Êúü - ÊåÇËΩΩÂÆåÊàêÔºàËÆøÈóÆDOMÂÖÉÁ¥†Ôºâ\r\n  mounted() {\r\n    this.getVocde();\r\n  },\r\n  destroyed() {\r\n    clearTimeout(this.register.tomer);\r\n  }\r\n};\r\n</script>\r\n<style scoped lang='less'>\r\n/* @import url(); ÂºïÂÖ•cssÁ±ª */\r\n.form-item {\r\n  padding-right: 30px;\r\n}\r\n.vcode {\r\n  user-select: none;\r\n  .el-input {\r\n    width: 35%;\r\n    float: left;\r\n  }\r\n  div.svg {\r\n    width: 35%;\r\n    float: left;\r\n    height: 40px;\r\n    /deep/ svg {\r\n      width: 100% !important;\r\n      height: 100% !important;\r\n    }\r\n  }\r\n  .refresh {\r\n    width: 30%;\r\n    height: 40px;\r\n    float: left;\r\n    line-height: 50px;\r\n    color: blue;\r\n    cursor: pointer;\r\n  }\r\n}\r\n</style>"]}]}