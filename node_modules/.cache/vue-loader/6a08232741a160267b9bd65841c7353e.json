"\",\r\n        phone: \"\",\r\n        phonevcode: \"\"\r\n      },\r\n      //Ë°®ÂçïÈ™åËØÅÊï∞ÊçÆ\r\n      rules: {\r\n        user: [\r\n          //triggerÂ§±ÂéªÁÑ¶ÁÇπËß¶Âèë\r\n          // { required: true, message: \"ËØ∑ËæìÂÖ•Áî®Êà∑Âêç\", trigger: \"blur\" },\r\n          {\r\n            //Êï∞ÊçÆÁ±ªÂûã\r\n            type: \"string\",\r\n            //Ê≠£ÂàôËßÑÂàô\r\n            required: true,\r\n            pattern: /^[\\w\\u4e00-\\u9fa5\\uac00-\\ud7ff\\u0800-\\u4e00\\-]{2,9}$/,\r\n            message: \"2-9‰ΩçÔºåÊï∞Â≠óÔºåÂ≠óÊØçÔºå_ -‰∏≠Êó•Èü©Êñá‚ù§\",\r\n            trigger: [\"blur\", \"change\"]\r\n          }\r\n        ],\r\n        password: [\r\n          {\r\n            validator: (rule, value, callback) => {\r\n              if (value) {\r\n                if (\r\n                  /^[\\w<>,.?|;':\"{}!@#$%^&*()\\/\\-\\[\\]\\\\]{6,18}$/.test(value)\r\n                ) {\r\n                  callback();\r\n                } else {\r\n                  callback(new Error(\"ÂØÜÁ†Å6-18‰Ωç,‰∏çÂÖÅËÆ∏Âá∫Áé∞Â•áÊÄ™ÁöÑÂ≠óÁ¨¶üòé\"));\r\n                }\r\n              } else {\r\n                callback(new Error(\"ËØ∑ËæìÂÖ•ÂØÜÁ†Å\"));\r\n              }\r\n              //Âú®ËøôÈáåËøòÈúÄË¶ÅÈ™åËØÅÁ°ÆËÆ§ÂØÜÁ†ÅÁöÑËÆ§ËØÅ\r\n              /**\r\n               * Â¶ÇÊûúÂú®vueÈáåÈù¢ÊÉ≥Ë¶ÅÊìç‰ΩúDOMÁöÑËØù\r\n               * Â∞±ÁªôÁà∂Á∫ßÊ†áÁ≠æ ËÆæÁΩÆ ref=\"name\"\r\n               * ÁÑ∂ÂêéÈÄöËøáthis.$refs.nameÁöÑÊñπÂºèËé∑Âèñ\r\n               * */\r\n\r\n              this.form.checkpassword &&\r\n                this.$refs.form.validateField(\"checkpassword\");\r\n            },\r\n            type: \"string\",\r\n            message: \"6-18‰ΩçÔºå‰∏çÂÖÅËÆ∏Âá∫Áé∞Â•áÊÄ™ÁöÑÂ≠óÁ¨¶üòé\",\r\n            trigger: [\"blur\", \"change\"],\r\n            required: true\r\n          }\r\n        ],\r\n        //ÊâãÊú∫Âè∑È™åËØÅ\r\n        phone: [\r\n          {\r\n            validator(rule, value, callback) {\r\n              if (value) {\r\n                let Reg = /^